<Application x:Class="GPR5100_S2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GPR5100_S2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Stone Image -->
        <BitmapImage x:Key="stoneImage" UriSource="pack://siteoforigin:,,,/Images/Stone.png" />

        <!-- Grass Image -->
        <BitmapImage x:Key="grassImage" UriSource="pack://siteoforigin:,,,/Images/Grass.png"/>

        <!-- Wood Image -->
        <BitmapImage x:Key="woodImage" UriSource="pack://siteoforigin:,,,/Images/Wood.png"/>

        <!-- Ice Image -->
        <BitmapImage x:Key="iceImage" UriSource="pack://siteoforigin:,,,/Images/Ice.png"/>

        <!-- Plastic Image -->
        <BitmapImage x:Key="plasticImage" UriSource="pack://siteoforigin:,,,/Images/Plastic.png"/>
        
        <!-- Fill Image -->
        <BitmapImage x:Key="fillImage" UriSource="pack://siteoforigin:,,,/Images/Fill.png"/>
    </Application.Resources>
</Application>
